<core:FragmentDefinition
        xmlns:vhd="sap.ui.comp.valuehelpdialog"
		xmlns:core="sap.ui.core"
		xmlns:fb="sap.ui.comp.filterbar"
		xmlns:zlib="by.mda.bn.z_ehs_library_1"
		xmlns="sap.m"
        xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <fb:FilterBar 
            useToolbar="false"
            search="onSearch"
            filtersDialogCancel="onCancel"
            showRestoreButton="false"
            filterBarExpanded="true"
            showClearOnFB="true"
            clear="onClearFilter"
            filtersDialogBeforeOpen="handelFilterDialogOpen">
        <fb:filterGroupItems>
            <fb:FilterGroupItem
                    groupName="firstGroup"
                    name="CR_DATE"
                    label="{i18n>dateColumn}"
                    visibleInFilterBar="true">
                <fb:control>
                    <zlib:MultiInput 
							valueHelpTitle="{i18n>dateColumn}"
							valueHelpKey="CR_DATE"
							valueHelpKeyType="date"
							valueHelpDescriptionKey="{i18n>dateColumn}"
                            valueHelpOnly="true"
                            rangesOnly="true"
                            listPath="/CR_DATE"
                            beforeOpen="._prepareSelectionModel"
							tokensBindingInfo="{
								modelName: 'filterModel',
								bindingPath: '/CR_DATE'
							}">
                    </zlib:MultiInput>
                </fb:control>
            </fb:FilterGroupItem>
